{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended",
      "tslint-react"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "//"
      }
    ]
  },
  "rulesDirectory": []
}
