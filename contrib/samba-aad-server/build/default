# Default server configuration
#
server {
	listen 80 default_server;
	listen [::]:80 default_server;

	server_name _domaininfo_;

	location / {
		include uwsgi_params;
        #uwsgi_pass unix:/infosrv/uwsgi/uwsgi.sock;
		uwsgi_pass 127.0.0.1:8988;
	}

}
